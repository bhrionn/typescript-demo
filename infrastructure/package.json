{
  "name": "infrastructure",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:security": "jest tests/security-checklist.test.ts",
    "validate:security": "./scripts/validate-security.sh",
    "cdk": "cdk",
    "lint": "eslint \"lib/**/*.ts\" \"bin/**/*.ts\" --no-error-on-unmatched-pattern",
    "lint:fix": "eslint \"lib/**/*.ts\" \"bin/**/*.ts\" --fix --no-error-on-unmatched-pattern",
    "format": "prettier --write \"**/*.{ts,json,md}\""
  },
  "dependencies": {
    "aws-cdk-lib": "^2.110.0",
    "constructs": "^10.3.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.10.0",
    "@types/source-map-support": "^0.5.10",
    "aws-cdk": "^2.110.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "source-map-support": "^0.5.13",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.47.0"
  }
}
