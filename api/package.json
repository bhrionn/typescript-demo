{
  "name": "api",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint \"src/**/*.ts\" --no-error-on-unmatched-pattern",
    "lint:fix": "eslint \"src/**/*.ts\" --fix --no-error-on-unmatched-pattern",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "migrate:up": "ts-node migrations/run-migrations.ts",
    "migrate:down": "ts-node migrations/run-migrations.ts rollback"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.470.0",
    "@aws-sdk/s3-request-presigner": "^3.470.0",
    "@aws-sdk/client-secrets-manager": "^3.470.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.470.0",
    "pg": "^8.11.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.130",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.0",
    "prettier": "^3.1.0"
  }
}
